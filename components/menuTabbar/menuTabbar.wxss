/* components/menuTabbar/menuTabbar.wxss */
.menuTabbar-main{
  position: fixed;
  bottom: 0;
  background-color: #481e6e;
  width: 100%;
  height: 100rpx;
  z-index: 99;
}
.tabbar-item{
  
}
.tabbar-main-another{
  position: fixed;
  bottom: 100rpx;
  background-color: #6e691e00;
  width: 100%;
  height: 100rpx;
  z-index: 100;
  display: flex;
  justify-content: center;
}
.tabbar-leftMenu{
  width: 50vw;
  height: 70rpx;
  background-color: rgba(236, 43, 9, 0);
  border-bottom-right-radius: 120rpx 100rpx;
  z-index: 101;
  border-bottom: 5rpx solid #000000;
}
.tabbar-bigMenu{
  width: 150rpx;
  height: 100%;
  background-color: rgb(255, 255, 255);
  border-top-left-radius: 150rpx 150rpx;
  border-top-right-radius: 150rpx 150rpx;
  z-index: 109;
  position: absolute;
  bottom: -50rpx;
  border-top: 5rpx solid #000000;
}
.tabbar-rightMenu{
  /* 自动计算宽度进行适配 */
  width: 50vw;
  height: 70rpx;
  background-color: rgba(72, 204, 20, 0);
  border-bottom-left-radius: 120rpx 100rpx;
  z-index: 101;
  border-bottom: 5rpx solid #000000;
}