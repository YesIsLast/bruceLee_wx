<!--pages/mapDemo/mapDemo.wxml-->
<view>
  <map id="myMap" style="width: 100%; height: 500px;" latitude="{{latitude}}" longitude="{{longitude}}"
    markers="{{markers}}" polyline="{{polyline}}" show-location></map>
  <view>
    <button type="primary" bindtap="mapMoving">开始执行</button>
    <!-- <button type="primary" bindtap="mapMoving">自定义分段移动</button>
    <button type="primary" bindtap="mapMoving">小程序平滑移动</button> -->
  </view>
  <!-- 底部tabbar组件 -->
  <!-- <menuTabbar current="{{1}}"></menuTabbar> -->
  <custom_tabbar current="{{1}}"></custom_tabbar>
</view>